		instance.OnServerEvent:Connect(function(...)
			player, a, b = ...
		end)

		remote("test", 1)
		expect(player).to.be.ok()
		expect(a).to.equal("test")
		expect(b).to.equal(1)
	end)

	it("should promise an event", function()
