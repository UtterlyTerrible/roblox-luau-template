		test1, test2, arg1, arg2 = nil
		remote.test:disconnectAll()
		remote:fire("test", 1)

		expect(test1).to.never.be.ok()
		expect(test2).to.never.be.ok()
		expect(arg1).to.equal("test")
		expect(arg2).to.equal(1)
