		asyncRemote:destroy()

		expect(function()
			asyncRemote:request("", 1):expect()
		end).to.throw()

		expect(function()
			instance:InvokeClient(player, "", 1)
		end).to.throw()

		expect(function()
			asyncRemote:onRequest(function(): () end)
		end).to.throw()
	end)

	it("should apply the middleware", function()
		local middlewareRemote, arg1, arg2, result

