		producer:subscribe(function()
			calls += 1
			if calls == 1 then
				producer:subscribe(function()
					calls += 1
				end)
			end
		end)

		producer:flush()
		expect(calls).to.equal(0)

		producer.increment(1)
		producer:flush()
		expect(calls).to.equal(1)

